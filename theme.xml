  /*****************************
  * Theme Name : Bulger
  * Designer : Nurahmat
  * Version : 1.0.1 beta
  * Description : This theme based modern css framework called bulma
  ******************************/
  .rounded {
    border-radius: 3px;
    overflow: hidden;
    }
  .sharing {
    display: none;
    }
  .hero.is-dark .hero-body a {
    color: white;
  }
  .layout-popularposts {}
  .layout-popularposts .item {
    margin-bottom: 17px;
    }
  .post-body {
    margin-top: 15px;
    }
  .separator {
    
  }
  .separator > a {
    margin: 0px;
  }
  .separator img {
    display: block;
    margin-right: auto;
    margin-left: auto;
    width: 100%;
    }
  pre {
    white-space: pre-line !important;
    }
  .post.card:hover {
      box-shadow: 0px 0px 0px 0px rgba(0,0,0,0.1);
      transition: .5s;
  }
  @media (min-width: 768px) {
    .separator img {
      max-width: 100%;
      }
  }
  .wrapper-profile {
    max-width: 500px;
    margin-left: auto;
    margin-right: auto;
  }
